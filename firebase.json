{
  "hosting": [{
        "site": "lightningweb",
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "public": ".",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ]
    }, {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }]
}
